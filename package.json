{
  "name": "weightlossprojectlab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "node server.js",
    "setup:https": "powershell -ExecutionPolicy Bypass -File ./setup-https.ps1",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" next build --webpack",
    "build:debug": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" NEXT_DEBUG_BUILD=1 next build",
    "build:fast": "cross-env NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:notifications": "tsx scripts/test-notifications.ts",
    "test:notifications:quick": "tsx scripts/create-test-notification.ts",
    "test:notifications:skip-email": "SKIP_EMAIL_TESTS=true tsx scripts/test-notifications.ts",
    "test:notifications:no-cleanup": "SKIP_CLEANUP=true tsx scripts/test-notifications.ts",
    "migrate:duties": "tsx scripts/migrate-duties-to-households.ts",
    "migrate:duties:live": "tsx scripts/migrate-duties-to-households.ts --live",
    "migrate:duties:rollback": "tsx scripts/migrate-duties-to-households.ts --rollback",
    "validate:migration": "tsx scripts/validate-migration.ts",
    "bundle:size": "size-limit",
    "build:mobile": "cross-env CAPACITOR_BUILD=true NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "cap:sync": "npm run build:mobile && npx cap sync",
    "cap:sync:ios": "npm run build:mobile && npx cap sync ios",
    "cap:sync:android": "npm run build:mobile && npx cap sync android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:run:ios": "npm run cap:sync:ios && npx cap run ios",
    "cap:run:android": "npm run cap:sync:android && npx cap run android",
    "generate-jobs": "tsx scripts/generate-jobs.ts",
    "generate-jobs-ml": "tsx scripts/ml/generate-jobs-ml.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@aparajita/capacitor-biometric-auth": "^9.1.2",
    "@capacitor/app": "^7.1.1",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/motion": "^7.0.1",
    "@capacitor/preferences": "^7.0.3",
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.10.0",
    "@sendgrid/mail": "^8.1.6",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.7",
    "@vercel/speed-insights": "^1.3.1",
    "@zxing/library": "^0.21.3",
    "autoprefixer": "^10.4.21",
    "browser-image-compression": "^2.0.2",
    "capacitor-health": "^7.0.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dompurify": "^3.3.1",
    "eslint": "^9.18.0",
    "eslint-config-next": "15.5.0",
    "firebase": "^11.0.2",
    "firebase-admin": "^12.6.0",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.555.0",
    "next": "^16.0.7",
    "next-nprogress-bar": "^2.4.7",
    "openai": "^6.9.0",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^3.11.174",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.0",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^10.1.0",
    "react-swipeable": "^7.0.1",
    "recharts": "^3.3.0",
    "remark-gfm": "^4.0.1",
    "resend": "^6.2.2",
    "stripe": "^20.0.0",
    "swr": "^2.3.6",
    "tailwindcss": "^3.4.14",
    "tesseract.js": "^5.1.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/eslintrc": "^3.3.1",
    "@netlify/functions": "^5.1.2",
    "@playwright/test": "^1.49.1",
    "@size-limit/file": "^11.2.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^30.0.0",
    "@types/uuid": "^10.0.0",
    "baseline-browser-mapping": "^2.9.19",
    "critters": "^0.0.23",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "size-limit": "^11.2.0",
    "typescript": "5.9.3"
  },
  "size-limit": [
    {
      "path": ".next/static/chunks/*.js",
      "limit": "200 KB"
    }
  ]
}
